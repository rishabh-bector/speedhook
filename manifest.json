{
    "name": "SpeedHook",
    "version": "1.0",
    "description": "Speed up embedded VideoJS players!",
    "manifest_version": 2,
    "permissions": ["activeTab", "declarativeContent", "storage"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "page_action": {
        "default_popup": "popup.html"
    }
  }